        android:textColor="#000000"
        android:textSize="@dimen/b7" />

    <TextView
        android:id="@+id/couleurFil1"
        android:layout_width="@dimen/c2"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:textColor="#000000"
        android:textSize="@dimen/b7" />

    <TextView
        android:id="@+id/numeroBorne"
        android:layout_width="@dimen/c2"
        android:layout_height="wrap_content"
