<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="${relativePackage}.${activityClass}" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:text="@string/menuMagasinier" />
    
     <ImageButton
        android:id="@+id/infoButton1"
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_margin="@dimen/forT"
        android:src="@drawable/aide3" />
    
        <ImageButton
        android:id="@+id/exitButton1"
        android:layout_width="40dip"
        android:layout_height="40dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_margin="@dimen/forT"
        android:src="@drawable/exit" />

         <LinearLayout
        android:id="@+id/head"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/titleTab"
        android:layout_centerHorizontal="true"
        android:layout_toLeftOf="@id/exitButton1"
        android:layout_toRightOf="@id/infoButton1" >

        <TextView
            android:id="@+id/textView9"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:text="@string/ordreOperation"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#0000ff"
            android:textSize="@dimen/forTMin" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:text="@string/operationsRealiser"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#0000ff"
            android:textSize="@dimen/forTMin" />

      
        
       

    </LinearLayout>

             <TextView
                 android:id="@+id/titleTab"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_below="@+id/textView1"
                 android:layout_centerHorizontal="true"
                 android:layout_margin="@dimen/forT"
                 android:text="@string/ordreJour"
                 android:textSize="@dimen/forT" />
    
    <GridView
        android:id="@+id/gridview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/imageButton1"
        android:layout_alignRight="@id/head"
        android:layout_below="@+id/head"
        android:layout_centerHorizontal="true"
        android:layout_margin="5dp"
        android:layout_toLeftOf="@id/exitButton1"
        android:layout_toRightOf="@id/infoButton1"
        android:numColumns="1"
        android:stretchMode="columnWidth" >

    </GridView>

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/exitButton1"
        android:layout_toLeftOf="@+id/exitButton1"
        android:src="@drawable/valider9" />

</RelativeLayout>
