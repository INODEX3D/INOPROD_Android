<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="${relativePackage}.${activityClass}" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:text="Menu principal profil magasiniers"
 />
    
     <ImageButton
        android:id="@+id/infoButton1"
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_margin="@dimen/b12"
        android:src="@drawable/aide3" />
    
        <ImageButton
        android:id="@+id/exitButton1"
        android:layout_width="40dip"
        android:layout_height="40dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_margin="@dimen/b12"
        android:src="@drawable/exit" />

         <LinearLayout
        android:id="@+id/head"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/titleTab"
        android:layout_centerHorizontal="true"
        android:layout_toLeftOf="@id/exitButton1"
        android:layout_toRightOf="@id/infoButton1" >

        

        <TextView
            android:id="@+id/textView9"
            android:layout_width="44dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:text="Ordre des opérations"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#0000ff"
            android:textSize="@dimen/b1" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:text="Opérations à réaliser"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#0000ff"
            android:textSize="@dimen/b1" />

      
        
       

    </LinearLayout>

             <TextView
                 android:id="@+id/titleTab"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_below="@+id/textView1"
                 android:layout_centerHorizontal="true"
                 android:layout_margin="@dimen/b12"
                 android:text="Ordre du jour"
                 android:textSize="@dimen/b9" />
    
    <GridView
        android:id="@+id/gridview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/imageButton1"
        android:layout_alignRight="@id/head"
        android:layout_below="@+id/head"
        android:layout_centerHorizontal="true"
        android:layout_margin="5dp"
        android:layout_toLeftOf="@id/exitButton1"
        android:layout_toRightOf="@id/infoButton1"
        android:numColumns="1"
        android:stretchMode="columnWidth" >

    </GridView>

</RelativeLayout>
