<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/statutLiaison"
        android:layout_width="@dimen/c5"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:textColor="#000000"
        android:textSize="@dimen/b9" />

    <TextView
        android:id="@+id/numeroRevisionLiaison"
        android:layout_width="@dimen/c5"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:textColor="#000000"
        android:textSize="@dimen/b9" />

    <TextView
        android:id="@+id/typeCable"
        android:layout_width="@dimen/c2"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:textColor="#000000"
        android:textSize="@dimen/b9" />

    <TextView
        android:id="@+id/numeroFil"
        android:layout_width="@dimen/c5"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:textColor="#000000"
        android:textSize="@dimen/b9" />

    <TextView
        android:id="@+id/numeroFilDansCable"
        android:layout_width="@dimen/c5"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:textColor="#000000"
        android:textSize="@dimen/b9" />

    <TextView
        android:id="@+id/couleurFil"
        android:layout_width="@dimen/c5"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:textColor="#000000"
        android:textSize="@dimen/b9" />

    <TextView
        android:id="@+id/numeroBorne"
        android:layout_width="@dimen/c5"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:textColor="#000000"
        android:textSize="@dimen/b9" />

</LinearLayout>